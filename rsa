#!/usr/bin/python3

"""Find prime factors of a number"""
import sys
import time


def is_prime(n):
    """Check if a number is prime"""
    if n <= 1:
        return False
    if n <= 3:
        return True
    if n % 2 == 0 or n % 3 == 0:
        return False
    i = 5
    while i * i <= n:
        if n % i == 0 or n % (i + 2) == 0:
            return False
        i += 6
    return True

def prime_factors(n):
    """Find prime factors of a number"""
    factors = []
    while n % 2 == 0:
        factors.append(2)
        n //= 2
    for i in range(3, int(n**0.5) + 1, 2):
        while n % i == 0:
            factors.append(i)
            n //= i
    if n > 2:
        factors.append(n)
    return factors

def factorize_file(path):
    """Factorize a number into its prime factors"""
    try:
        with open(path) as f:
            for line in f:
                n = int(line.strip())
                factors = prime_factors(n)
                if len(factors) == 2 and is_prime(factors[0]) and is_prime(factors[1]):
                    print(f"{n}={factors[0]}*{factors[1]}")
                else:
                    print("Unable to find two prime factors.")
                if time.time() - start_time > 5:
                    print("Execution time exceeded the limit.")
                    break
    except FileNotFoundError:
        print(f"File '{path}' not found.")
    except ValueError:
        print("Invalid data in the file.")

if __name__ == "__main__":
    if len(sys.argv) != 2:
        sys.exit(f"Usage: {sys.argv[0]} <path>")

    factors = sys.argv[1]

    start_time = time.time()

    factorize_file(factors)
