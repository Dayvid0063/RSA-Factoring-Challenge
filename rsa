#!/usr/bin/python3

import sys
import time
import random

def gcd(a, b):
    while b:
        a, b = b, a % b
    return a

def rho_pollard(n):
    if n % 2 == 0:
        return 2

    x = random.randint(1, n-1)
    y = x
    c = random.randint(1, n-1)
    d = 1

    f = lambda x: (x ** 2 + c) % n

    while d == 1:
        x = f(x)
        y = f(f(y))
        d = gcd(abs(x - y), n)

    return d if d != n else None

def factorize(n):
    factors = []
    while n > 1:
        factor = rho_pollard(n)
        if factor:
            factors.append(factor)
            n //= factor
        else:
            factors.append(n)
            break
    return factors

def factorize_file(path):
    try:
        with open(path) as f:
            for line in f:
                n = int(line.strip())
                factors = factorize(n)
                if len(factors) == 2 and all(is_prime(factor) for factor in factors):
                    print(f"{n}={factors[0]}*{factors[1]}")
                else:
                    print("Unable to find two prime factors.")
                if time.time() - start_time > 5:
                    print("Execution time exceeded the limit.")
                    break
    except FileNotFoundError:
        print(f"File '{path}' not found.")
    except ValueError:
        print("Invalid data in the file.")

def is_prime(n):
    if n <= 1:
        return False
    if n <= 3:
        return True
    if n % 2 == 0 or n % 3 == 0:
        return False
    i = 5
    while i * i <= n:
        if n % i == 0 or n % (i + 2) == 0:
            return False
        i += 6
    return True

if __name__ == "__main__":
    if len(sys.argv) != 2:
        sys.exit(f"Usage: {sys.argv[0]} <path>")

    factors = sys.argv[1]

    start_time = time.time()

    factorize_file(factors)
